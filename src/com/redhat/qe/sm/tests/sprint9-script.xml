<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >

<suite name="SMTestSuite">

	<test name="subscription-manager Tests">
		<classes>
			<class name="com.redhat.qe.sm.tests.GeneralTests" />	
			<class name="com.redhat.qe.sm.tests.Register" />
			<class name="com.redhat.qe.sm.tests.List" />			
			<class name="com.redhat.qe.sm.tests.Subscribe" />	
			<class name="com.redhat.qe.sm.tests.Unsubscribe" />
			<class name="com.redhat.qe.sm.tests.Unregister" />
		</classes>
		<groups>
			<run>
				<include name="sm_setup" />
				<include name="sprint9-script" />
				<exclude name="not_implemented" />
			</run>
		</groups>
	</test>
	
	<listeners>
  		<listener class-name="com.redhat.qe.auto.selenium.TestNGListener" />
  		<listener class-name="com.redhat.qe.auto.bugzilla.BugzillaTestNGListener" />
	</listeners>
	
</suite>